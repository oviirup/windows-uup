{
  "name": "windows-uup",
  "version": "0.1.3",
  "type": "module",
  "author": {
    "name": "Avirup Ghosh",
    "url": "https://github.com/oviirup"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "npx rimraf .turbo .next",
